{
    "manifest_version": 2,
    "name": "TD helper",
    "version": "0.0.1",
  
    "description": "Browser extension that shows how much time you have logged in TD",
    "background": {
        "scripts": ["background.js"],
        "persistent": true
      },
    "author": "Sergey Tatsiak",
    
    "incognito": "not_allowed",

    "permissions": ["*://*.timedoctor.com/*", "tabs"],

    "browser_action": {},

    "web_accessible_resources": [
      "sounds/*"
  ]
  }